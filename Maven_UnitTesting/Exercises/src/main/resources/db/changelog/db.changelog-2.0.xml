<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">

    <changeSet id="changelog-2.0-adrian" author="adrian">
        <createTable tableName="ADDRESS-Adrian">
            <column name="ADR_ID" type="number">
                <constraints primaryKey="true"></constraints>
            </column>
            <column name="STREET" type="varchar2(40)"></column>
            <column name="NO" type="number(6)"></column>
            <column name="CITY" type="varchar2(40)">
                <constraints nullable="false"></constraints>
            </column>
        </createTable>
    </changeSet>

</databaseChangeLog>